{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },

  "ConnectionStrings": {
    "Default": "*****"
  },

  "Passwords": {
    "Iterations": 500000
  },

  "DataProtection": {
    "AppDiscriminator": "*****",
    "KeysFolderPath": "*****"
  },

  "PaypalApi": {
    "ClientTimeoutInSecs": 10,
