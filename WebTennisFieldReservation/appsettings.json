{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },

  "Passwords": {
    "Iterations": 500000
  },

  "MailSender": {

    "HostName": "*****",
    "Port": 587,
    "UseSSL": true,
    "User": "*****",
    "PasswordFileName": "*****"
  },

  "ConnectionStrings": {
    "Default": "*****"
  },

  "DataProtection": {
    "AppDiscriminator": "*****",
    "KeysFolderPath": "*****"
  },

  "TokenManager": {
    "EmailConfirmationValidTimeSpanInMinutes": 5,
    "PwdResetValidTimeSpanInMinutes": 5
  },

  "AuthenticationSchemes": {
    "MyAuthScheme": {
      "CookieMaxAgeInMinutes": 60
    }
  },

  "LoggedRecentlyPolicy": {
    "MaxAgeInMins": 1
  },

  "AllowedHosts": "*"
}
